    "prepublish": "npm run tsc"
  },
  "repository": "ratson/link-dir.git",
  "author": {
    "name": "Zoltan Kochan",
    "email": "zoltan.kochan@gmail.com",
    "url": "http://kochan.io"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^14.0.4",
    "@types/fs-extra": "^9.0.1",
    "cash-rm": "^0.2.0",
    "mos": "^1.3.1",
    "mos-plugin-readme": "^1.0.4",
    "typescript": "^3.9.3"
  },
  "dependencies": {
    "bole": "^4.0.0",
    "del": "^5.1.0",
    "fs-extra": "^9.0.0"
  },
  "mos": {
    "plugins": [
      "readme"
    ],
    "installation": {
