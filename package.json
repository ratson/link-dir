{
  "name": "link-dir",
  "version": "2.0.0",
  "description": "Link directory",
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "typings": "lib/index.d.ts",
  "bin": "lib/cli.js",
  "keywords": [
    "link",
    "linking",
    "hardlink",
    "hardlinking",
    "directory",
    "dir",
    "folder"
  ],
  "scripts": {
    "test": "npm run tsc && mos t",
    "md": "mos",
    "tsc": "rm -rf dist && tsc",
    "prepublish": "npm run tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zkochan/link-dir.git"
  },
  "author": {
    "name": "Zoltan Kochan",
    "email": "zoltan.kochan@gmail.com",
    "url": "http://kochan.io"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zkochan/link-dir/issues"
  },
  "homepage": "https://github.com/zkochan/link-dir#readme",
  "devDependencies": {
    "cash-rm": "^0.2.0",
    "mos": "^1.3.1",
    "mos-plugin-readme": "^1.0.4",
    "typescript": "^2.2.0"
  },
  "dependencies": {
    "@types/mz": "0.0.30",
    "@types/node": "^6.0.45",
    "bole": "^3.0.2",
    "graceful-fs": "^4.1.11",
    "mkdirp-promise": "^5.0.1",
    "mz": "^2.4.0",
    "rimraf-then": "^1.0.1"
  },
  "mos": {
    "plugins": [
      "readme"
    ],
    "installation": {
      "useShortAlias": true
    }
  },
  "engines": {
    "node": ">=4"
  }
}
